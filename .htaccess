RewriteEngine On
RewriteCond %{SERVER_PORT} 80
RewriteCond %{HTTP_HOST} ^(www\.)?signumsolutions\.tech
RewriteRule ^(.*)$ https://www.signumsolutions.tech/public/$1 [R,L]

# the following prevents display of the filetohide file
<files .env>
order allow,deny
deny from all
</files>


